class base1
{
public:
	base1()
	{
		m_a = 10;
	}
public:
	int m_a;
};

class base2
{
public:
	base2()
	{
		m_a = 20;
		//m_b = 20;
	}
public:
	int m_a;
	//int m_b;
};

//多继承
class son :public base1, public base2
{
public:
	int m_c;
	int m_d;

};

void test1()
{
	cout << sizeof(son) << endl;

	son s1;
	//cout << s1.m_a << endl; base2 int m_b	
//	s1.m_a //多进程中很容易引发二义性
	cout << s1.base1::m_a << endl; 
	cout << s1.base2::m_a << endl; 
}
