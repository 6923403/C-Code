//通过地址找到偏移量

//内部工作原理

void test2()

{

sheepostrich so;

so.m_age = 100;

//找到sheep的偏移量操作

cout << *(int *)((int *)*(int *)&so + 1) << endl;

//找到ostrich的偏移量

cout << *(int *)((int *)*((int *) &so +1)+1 ) << endl;

//输出age

cout << ((animal *)((char *)&so + *(int *)((int *)*(int *)&so +1)))<< endl;

}
