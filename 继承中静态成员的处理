class base

{

public:

static void func()//(int e)

{

cout << "base func" << endl;

}

static int m_a;

};

int base::m_a = 29;

class son :public base

{

public:

static void func()

{

cout << "son func" << endl;

}

static int m_a;

};

int son::m_a = 39;

void test1()

{

cout << base::m_a << endl; //静态成员属性 子类可以继承下来

cout << son::m_a << endl;

base::func();

son::func();

//	son::base::func(6);

}
