
#include <vector>

//#include <algorithm>

#include <numeric>

#include <iterator>

using namespace std;

void test1() //计算容器元素累计总和

{

    vector<int> q;

    for(int i = 0; i < 10; i++)

    {

        q.push_back(i);

    }

    int num = accumulate(q.begin(), q.end(), 1000); //第三个参数起始累加值

    cout << num << endl;

}

void test2() //向容器中添加元素

{

    vector<int> s;

    s.resize(10);

    fill(s.begin(), s.end(), 234);

    copy(s.begin(), s.end(), ostream_iterator<int>(cout, "  " ));



}
