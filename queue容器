
#include <queue>

using namespace std;

void test1()

{

    queue<int> w;

    w.push(5); //队尾添加

    w.push(15);

    w.push(23);

    w.push(27);

    while(!w.empty())

    {

        cout << w.front() << endl; //返回第一个元素 5

        cout << w.back() << endl; //返回最后一个元素 27

        w.pop(); //弹出队头元素

    }

    cout << w.size() << endl;

}

