//中断 按下操作返回

bool mylabel::event(QEvent * e)

{

    if(e -> type() == QEvent::MouseButtonPress)

    {

        QMouseEvent * ev = static_cast(e);

        QString str = QString("EVENT, 鼠标按下了 ， x = %1,  y = %2").arg(ev -> x()).arg(ev -> y());

        qDebug() <<  str;

        return true;

    }

    //其他事件 让父类作默认处理

    return QLabel::event(e);

}
