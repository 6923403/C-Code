void test1()
{
    vector<int> v1(10);
    vector<int> v2{10};

    vector<int> v3(10, 1);
    vector<int> v4{10, 1};

    for(auto s : v1)
        cout << s << "  ";
    cout << endl;
    for(auto s2 : v2)
        cout << s2 << "  ";
    cout << endl;
    for(auto s3 : v3)
        cout << s3 << "   ";
    cout << endl;
    for(auto s4 : v4)
        cout << s4 << "   ";

}
