#include <iostream>
#include <string>
using namespace std;

bool lengthCompare(const string &, const string &);
bool (*pf)(const string &, const string &);

void test1();

int main(int argc, char **argv)
{
    test1();
}

bool lengthCompare(const string &str1, const string &str2)
{
    if(str1.size() > str2.size())
        return true;
    else 
        return false;

}

void test1()
{
    string s1 = "aaaa";
    string s2 = "bbbbb";

    pf = lengthCompare; //pf指向lengthCompare
    bool b1 = pf("aaaaaaa", "bbb");
    
    if(b1)
        cout << "true" << endl;
    else
        cout << "false" << endl;
}
